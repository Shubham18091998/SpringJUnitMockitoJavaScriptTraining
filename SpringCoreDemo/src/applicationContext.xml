<?xml version="1.0" encoding="UTF-8"?>
<beans
    xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
 	
 	<bean id="custObj" class="com.spring.apppackage.Customer">
 	<constructor-arg value="1"></constructor-arg>
 	<constructor-arg value="Anita"></constructor-arg>
 	<constructor-arg value="anita@xyz.com"></constructor-arg>
 	<constructor-arg value="9999999999"></constructor-arg>
 	</bean>
 	<bean id="membershipObj" class="com.spring.apppackage.MemberShip">
 	<constructor-arg value="101"></constructor-arg>
 	<constructor-arg value="Gold"></constructor-arg>
 	<constructor-arg value="2"></constructor-arg>
 	<constructor-arg ref="custObj"></constructor-arg>
 	</bean>
 	
 	
    
 
</beans>